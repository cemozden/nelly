{
  "name": "nelly",
  "version": "1.1.1",
  "description": "nelly rss feeder",
  "main": "build/EntryPoint.js",
  "scripts": {
    "test": "jest --watch",
    "testci": "jest --ci",
    "compile" : "tsc && node copy-assets.js",
    "start": "env-cmd -f .env.prod node .",
    "start-dev": "tsc && node copy-assets.js && env-cmd -f .env.dev nodemon node .",
    "package" : "rm -rf dist && NODE_ENV=production pkg build/EntryPoint.js --targets linux,macos,win --output dist/nelly --debug > /home/cemozden/pkg.log"
  },
  "repository": "https://github.com/cemozden/nelly",
  "author": "cemozden",
  "license": "MIT",
  "dependencies": {
    "@types/crc": "^3.4.0",
    "@types/ejs": "^3.0.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.0",
    "@types/node-cron": "^2.0.3",
    "@types/nodemon": "^1.19.0",
    "@types/request": "^2.48.4",
    "@types/rimraf": "^2.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/winston": "^2.4.4",
    "@types/xml2js": "^0.4.5",
    "better-sqlite3": "^6.0.1",
    "crc": "^3.8.0",
    "ejs": "^3.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "moment": "^2.24.0",
    "node-cron": "^2.0.3",
    "nodemon": "^2.0.2",
    "pkg": "^4.4.4",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.2",
    "winston": "^3.2.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "ts-jest": "^25.2.1"
  },
  "pkg": {
    "bin": "build/EntryPoint.js",
    "assets": "assets/**/*"
  }
