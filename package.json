{
  "name": "nelly",
  "version": "1.0.0-BETA",
  "description": "An RSS feeder written in TypeScript",
  "main": "public/electron.js",
  "scripts": {
    "start": "env-cmd -f .env.dev craco build && env-cmd -f .env.dev electron public/electron.js",
    "start_electron": "env-cmd -f .env.dev electron public/electron.js",
    "build": "env-cmd -f .env.prod craco build",
    "test": "craco test",
    "eject": "craco eject",
    "package": "env-cmd -f .env.prod craco build && electron-packager . --overwrite --platform=all --arch=x64 --out=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cemozden/nelly.git"
  },
  "keywords": [
    "nelly",
    "rssfeeder",
    "rss"
  ],
  "author": "Cem Ã–zden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cemozden/nelly/issues"
  },
  "homepage": ".",
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/better-sqlite3": "^5.4.0",
    "@types/crc": "^3.4.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/node-cron": "^2.0.2",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.9.1",
    "@types/request": "^2.48.4",
    "@types/rimraf": "^2.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/winston": "^2.4.4",
    "@types/xml2js": "^0.4.5",
    "better-sqlite3": "^5.4.3",
    "crc": "^3.8.0",
    "env-cmd": "^10.0.1",
    "node-cron": "^2.0.3",
    "npm-watch": "^0.6.0",
    "react": "^16.12.0",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "react-test-renderer": "^16.12.0",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "socket.io": "^2.3.0",
    "typescript": "~3.7.2",
    "winston": "^3.2.1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "electron": "^7.1.5",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build:electron": "src/**/*.*"
  }
}
